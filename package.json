  "bugs": {
    "url": "https://github.com/dignityofwar/digbot/issues"
  },
  "homepage": "https://github.com/dignityofwar/digbot#readme",
  "engines": {
    "node": "=10.0.0"
  },
  "dependencies": {
    "awilix": "^4.2.3",
    "axios": "^0.19.2",
    "bull": "^3.12.1",
    "config": "^3.2.5",
    "cron": "^1.8.2",
    "csv-stringify": "^5.3.6",
    "discord.js": "~11.5.1",
    "googleapis": "^45.0.0",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.8.10",
    "opusscript": "^0.0.7",
